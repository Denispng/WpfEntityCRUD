<Window x:Class="WpfEntityCRUD.MemberIdInputDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfEntityCRUD"
        mc:Ignorable="d"
        Title="Enter Member ID" Height="150" Width="300">
    <Grid>
        <TextBox Name="MemberIdTextBox" Width="200" Height="30" HorizontalAlignment="Center" VerticalAlignment="Center" />
        <Button Content="Submit" Width="80" Height="30" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,10" Click="SubmitButton_Click"/>
    </Grid>
</Window>
